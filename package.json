{
  "name": "har-to-mocks",
  "version": "0.0.0",
  "type": "module",
  "description": "Extract response from .har file and create JSON mocks for mock server.",
  "author": "Peter Knezek @peterknezek",
  "bin": {
    "har-to-mocks": "./bin/run"
  },
  "bugs": "https://github.com/peterknezek/har-to-mocks/issues",
  "dependencies": {
    "@oclif/core": "^4.0.0",
    "@oclif/table": "^0.5.1",
    "archy": "^1.0.0",
    "fs-extra": "^11.2.0",
    "tslib": "^2.8.1",
    "update-notifier": "^5.1.0"
  },
  "devDependencies": {
    "@oclif/plugin-help": "^6.2.20",
    "@oclif/test": "^4.0.0",
    "@types/archy": "^0.0.36",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.2",
    "@types/update-notifier": "^6.0.8",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitest/ui": "^2.1.8",
    "chai": "^4.5.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "oclif": "^4.16.3",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/peterknezek/har-to-mocks",
  "keywords": [
    "cli",
    "har",
    "mocks",
    "mock",
    "testing",
    "mocking",
    "requests",
    "connect-api-mocker"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "bin": "har-to-mocks",
    "dirname": "har-to-mocks",
    "commands": {
      "strategy": "single",
      "target": "./lib/index.js"
    },
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ]
  },
  "repository": "peterknezek/har-to-mocks",
  "scripts": {
    "prepack": "rm -rf lib && rm -rf lib_types && tsc -b",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:ui": "vitest --ui",
    "build": "tsc -b",
    "har-to-mocks": "bin/run",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts,tsx}\" --fix"
  },
  "types": "lib/index.d.ts",
  "release": {
    "branches": [
      "master",
      "next"
    ]
  }
}
